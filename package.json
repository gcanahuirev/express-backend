{
  "name": "express-backend",
  "version": "0.0.1",
  "description": "Boilerplate simple backend express",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "start": "cross-env NODE_ENV=development node dist/app.js",
    "dev": "cross-env NODE_ENV=development tsnd --respawn --transpile-only src/app.ts"
  },
  "author": "Giuseppe Canahuire",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.15.0",
    "pnpm": ">=8.0.0"
  }
}
