{
  "name": "express-backend",
  "version": "0.0.1",
  "description": "Boilerplate simple backend express",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "build:watch": "tsc -w",
    "build:run": "nodemon dist/app.js",
    "start:dev": "pnpm build && cross-env NODE_ENV=development concurrently \"pnpm build:watch\" \"pnpm build:run\"",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "start": "cross-env NODE_ENV=development node dist/app.js"
  },
  "author": "Giuseppe Canahuire",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}