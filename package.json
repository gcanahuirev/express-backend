{
  "name": "express-backend",
  "version": "0.0.1",
  "description": "Boilerplate simple backend express",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "start": "cross-env NODE_ENV=development node dist/app.js",
    "dev": "cross-env NODE_ENV=development tsnd --respawn --transpile-only src/app.ts",
    "postinstall": "npx prisma generate",
    "m:dev": "npx prisma migrate dev"
  },
  "author": "Giuseppe Canahuire",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@typegoose/typegoose": "^11.7.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pino": "^8.16.1",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mongoose": "7.6.3",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "mongoose": "7.6.3"
  },
  "engines": {
    "node": ">=20.5.1",
    "pnpm": ">=8.7.4"
  }
}
